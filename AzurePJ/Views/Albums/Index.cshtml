@model List<string>

<h2>MM 文件夹下的图片</h2>
<div style="display:flex; flex-wrap:wrap; gap:10px;">
    @foreach (var url in Model)
    {
        <div>
            <img src="@url" style="width:150px; height:150px; object-fit:cover; border:1px solid #ccc; border-radius:8px;" />
        </div>
    }
</div>
